@model IEnumerable<FinalTask.ViewModel.VmQuery_7>
@{
    ViewData["Title"] = "Запросы";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Запрос 7</h2>


<div class="row">
    <div class="col-md-12 ">
        <h5 class="m-3 text-center">
            Выполняет группировку по полю Год рождения в таблице АБИТУРИЕНТЫ. Для каждой группы определяет количество абитуриентов
        </h5>
      
        <table class="table table-hover table-striped table-sm table-responsive-md">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Year)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CountStudents)
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Year)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CountStudents)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>